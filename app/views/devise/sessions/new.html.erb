<div class="center focus-box">
  <h1>Log In</h1>

  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <table class="center">
      <tr>
        <td><%= f.email_field :email, autofocus: true, placeholder: 'Email' %></td>
      </tr>
      <tr>
        <td><%= f.password_field :password, autocomplete: "off", placeholder: 'Password' %></td>
      </tr>
      <tr><td><hr/></td></tr>
      <tr>
        <td><%= f.submit "Log in" %></td>
      </tr>
    </table>
  <% end %>

  <%= render "devise/shared/links" %>
</div>